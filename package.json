{
  "name": "movies-for-pixi",
  "version": "1.0.0",
  "description": "a list of movies that pixi can watch",
  "scripts": {
    "start": "eleventy --serve",
    "clean": "rimraf .parcel-cache dist _site",
    "build": "npm run clean && eleventy",
    "bundle": "parcel build _site/index.html _site/**/index.html _site/directors/**/index.html --no-source-maps"
  },
  "keywords": [
    "movies",
    "pixi"
  ],
  "author": "Olle Johannesson",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@mischnic/parcel-resolver-root": "^0.4.2",
    "lodash": "^4.17.21",
    "parcel": "^2.10.2",
    "rimraf": "^5.0.5"
  }
}
